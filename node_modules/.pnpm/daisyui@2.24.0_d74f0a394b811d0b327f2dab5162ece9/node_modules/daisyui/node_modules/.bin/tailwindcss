else
  export NODE_PATH="$NODE_PATH:/Users/epvalcord/Desktop/Projects/FrameworkTemplates/ReactViteTailwindDaisyUI/node_modules/.pnpm/tailwindcss@3.1.8/node_modules/tailwindcss/lib/node_modules:/Users/epvalcord/Desktop/Projects/FrameworkTemplates/ReactViteTailwindDaisyUI/node_modules/.pnpm/tailwindcss@3.1.8/node_modules/tailwindcss/node_modules:/Users/epvalcord/Desktop/Projects/FrameworkTemplates/ReactViteTailwindDaisyUI/node_modules/.pnpm/tailwindcss@3.1.8/node_modules:/Users/epvalcord/Desktop/Projects/FrameworkTemplates/ReactViteTailwindDaisyUI/node_modules/.pnpm/node_modules:/Users/epvalcord/Desktop/Projects/FrameworkTemplates/ReactViteTailwindDaisyUI/node_modules:/Users/epvalcord/Desktop/Projects/FrameworkTemplates/node_modules:/Users/epvalcord/Desktop/Projects/node_modules:/Users/epvalcord/Desktop/node_modules:/Users/epvalcord/node_modules:/Users/node_modules:/node_modules:/Users/epvalcord/Desktop/Projects/FrameworkTemplates/ReactViteTailwindDaisyUI/node_modules/.pnpm/daisyui@2.24.0_d74f0a394b811d0b327f2dab5162ece9/node_modules/daisyui/node_modules:/Users/epvalcord/Desktop/Projects/FrameworkTemplates/ReactViteTailwindDaisyUI/node_modules/.pnpm/daisyui@2.24.0_d74f0a394b811d0b327f2dab5162ece9/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../tailwindcss@3.1.8/node_modules/tailwindcss/lib/cli.js" "$@"
else
  exec node  "$basedir/../../../../../tailwindcss@3.1.8/node_modules/tailwindcss/lib/cli.js" "$@"
