#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/epvalcord/Desktop/Projects/FrameworkTemplates/ReactViteTailwind/node_modules/.pnpm/rollup@2.77.3/node_modules/rollup/dist/bin/node_modules:/Users/epvalcord/Desktop/Projects/FrameworkTemplates/ReactViteTailwind/node_modules/.pnpm/rollup@2.77.3/node_modules/rollup/dist/node_modules:/Users/epvalcord/Desktop/Projects/FrameworkTemplates/ReactViteTailwind/node_modules/.pnpm/rollup@2.77.3/node_modules/rollup/node_modules:/Users/epvalcord/Desktop/Projects/FrameworkTemplates/ReactViteTailwind/node_modules/.pnpm/rollup@2.77.3/node_modules:/Users/epvalcord/Desktop/Projects/FrameworkTemplates/ReactViteTailwind/node_modules/.pnpm/node_modules:/Users/epvalcord/Desktop/Projects/FrameworkTemplates/ReactViteTailwind/node_modules:/Users/epvalcord/Desktop/Projects/FrameworkTemplates/node_modules:/Users/epvalcord/Desktop/Projects/node_modules:/Users/epvalcord/Desktop/node_modules:/Users/epvalcord/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/epvalcord/Desktop/Projects/FrameworkTemplates/ReactViteTailwind/node_modules/.pnpm/rollup@2.77.3/node_modules/rollup/dist/bin/node_modules:/Users/epvalcord/Desktop/Projects/FrameworkTemplates/ReactViteTailwind/node_modules/.pnpm/rollup@2.77.3/node_modules/rollup/dist/node_modules:/Users/epvalcord/Desktop/Projects/FrameworkTemplates/ReactViteTailwind/node_modules/.pnpm/rollup@2.77.3/node_modules/rollup/node_modules:/Users/epvalcord/Desktop/Projects/FrameworkTemplates/ReactViteTailwind/node_modules/.pnpm/rollup@2.77.3/node_modules:/Users/epvalcord/Desktop/Projects/FrameworkTemplates/ReactViteTailwind/node_modules/.pnpm/node_modules:/Users/epvalcord/Desktop/Projects/FrameworkTemplates/ReactViteTailwind/node_modules:/Users/epvalcord/Desktop/Projects/FrameworkTemplates/node_modules:/Users/epvalcord/Desktop/Projects/node_modules:/Users/epvalcord/Desktop/node_modules:/Users/epvalcord/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin/rollup" "$@"
else
  exec node  "$basedir/../../dist/bin/rollup" "$@"
fi
