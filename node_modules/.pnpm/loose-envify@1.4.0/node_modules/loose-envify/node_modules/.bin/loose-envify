#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/epvalcord/Desktop/Projects/FrameworkTemplates/ReactViteTailwind/node_modules/.pnpm/loose-envify@1.4.0/node_modules/loose-envify/node_modules:/Users/epvalcord/Desktop/Projects/FrameworkTemplates/ReactViteTailwind/node_modules/.pnpm/loose-envify@1.4.0/node_modules:/Users/epvalcord/Desktop/Projects/FrameworkTemplates/ReactViteTailwind/node_modules/.pnpm/node_modules:/Users/epvalcord/Desktop/Projects/FrameworkTemplates/ReactViteTailwind/node_modules:/Users/epvalcord/Desktop/Projects/FrameworkTemplates/node_modules:/Users/epvalcord/Desktop/Projects/node_modules:/Users/epvalcord/Desktop/node_modules:/Users/epvalcord/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/epvalcord/Desktop/Projects/FrameworkTemplates/ReactViteTailwind/node_modules/.pnpm/loose-envify@1.4.0/node_modules/loose-envify/node_modules:/Users/epvalcord/Desktop/Projects/FrameworkTemplates/ReactViteTailwind/node_modules/.pnpm/loose-envify@1.4.0/node_modules:/Users/epvalcord/Desktop/Projects/FrameworkTemplates/ReactViteTailwind/node_modules/.pnpm/node_modules:/Users/epvalcord/Desktop/Projects/FrameworkTemplates/ReactViteTailwind/node_modules:/Users/epvalcord/Desktop/Projects/FrameworkTemplates/node_modules:/Users/epvalcord/Desktop/Projects/node_modules:/Users/epvalcord/Desktop/node_modules:/Users/epvalcord/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
